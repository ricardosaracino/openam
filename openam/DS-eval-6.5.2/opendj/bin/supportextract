OPENDJ_INVOKE_CLASS="org.opends.server.tools.supportextract.SupportExtract"
export OPENDJ_INVOKE_CLASS

SCRIPT_NAME="supportextract"
export SCRIPT_NAME

SCRIPT_DIR=`dirname "${0}"`
